# 테스트 환경 설정
NODE_ENV=test

# API 설정
EXPO_PUBLIC_API_URL=http://10.0.2.2:8000

# 앱 설정
EXPO_PUBLIC_APP_NAME=MyApp Test
EXPO_PUBLIC_APP_VERSION=1.0.0-test

# 디버그 설정
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=warn
EXPO_PUBLIC_DEV_TOOLS=false

# 네트워크 설정
EXPO_PUBLIC_API_TIMEOUT=5000
EXPO_PUBLIC_API_RETRY_ATTEMPTS=1
EXPO_PUBLIC_API_RETRY_DELAY=500

# 개발자 도구
EXPO_PUBLIC_RN_DEBUGGER=false
EXPO_PUBLIC_NETWORK_LOGGING=false
EXPO_PUBLIC_PERFORMANCE_MONITORING=false

# UI 설정
EXPO_PUBLIC_DEFAULT_THEME=light
EXPO_PUBLIC_ANIMATIONS_ENABLED=false
